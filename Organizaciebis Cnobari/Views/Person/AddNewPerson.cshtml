@model Organizaciebis_Cnobari.Models.Person

@{
    ViewData["Title"] = "ახალი მომხმარებლის დამატება";
}
<form asp-action="AddNewPerson" asp-controller="Person" enctype="multipart/form-data" style="margin-left:300px">
    <h6>ახალი მომხმარებლის დამატება</h6>
    <partial name="~/Views/Person/Helper/PersonForm.cshtml" model="Model"></partial>
    <button type="submit" class="btn btn-primary">დამატება</button>
</form>
@if (ViewBag.Message != null)
{
    <div class="alert alert-danger" role="alert">
        @ViewBag.Message
    </div>
}
